{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://moj.gov.uk/cpp/progression/command/progression.command.update-matched-defendant-custodial-information.json",
  "type": "object",
  "properties": {
    "caseId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "masterDefendantId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "defendants": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
      }
    },
    "custodialEstablishment": {
      "description": "The custodial establishment for Defendant",
      "$ref": "http://justice.gov.uk/core/courts/personDefendant.json#/definitions/custodyEstablishment"
    }
  },
  "required": [
    "caseId",
    "masterDefendantId",
    "defendants"
  ],
  "additionalProperties": false
}
