{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Command schema for notifying prison court register document failed",
  "id": "http://justice.gov.uk/core/courts/record-prison-court-register-failed.json",
  "type": "object",
  "properties": {
    "payloadFileId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "id": {
      "description": "The court centre reference data identifier",
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "templateIdentifier": {
      "type": "string"
    },
    "conversionFormat": {
      "type": "string"
    },
    "requestedTime": {
      "type": "string",
      "format": "date-time"
    },
    "failedTime": {
      "type": "string",
      "format": "date-time"
    },
    "courtCentreId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "prisonCourtRegisterStreamId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "originatingSource": {
      "type": "string"
    },
    "reason": {
      "type": "string"
    },
    "hearingDate": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": [
    "payloadFileId",
    "failedTime",
    "requestedTime",
    "templateIdentifier",
    "conversionFormat",
    "reason"
  ]
}