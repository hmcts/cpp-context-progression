{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/pet-form-finalised.json",
  "type": "object",
  "properties": {
    "petId": {
      "description": "Pet Id",
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "caseId": {
      "description": "Case Id",
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "prosecutionCase": {
      "description": "Prosecution Case",
      "$ref": "http://justice.gov.uk/core/courts/prosecutionCase.json#"
    },
    "userId": {
      "description": "Logged In User Id",
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "submissionId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "materialId": {
      "description": "Materail id associated with per form when it gets finalised",
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "finalisedFormData": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "caseId",
    "prosecutionCase",
    "petId",
    "userId",
    "finalisedFormData"
  ],
  "additionalProperties": false
}
