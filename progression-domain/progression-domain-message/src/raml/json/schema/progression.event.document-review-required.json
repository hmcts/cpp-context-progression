{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/progression.event.document-review-required.json",
  "type": "object",
  "properties": {
    "materialId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "receivedDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "documentId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "documentName": {
      "type": "string"
    },
    "caseId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "source": {
      "type": "string"
    },
    "urn": {
      "type": "string"
    },
    "prosecutingAuthority": {
      "type": "string"
    },
    "documentType": {
      "type": "string"
    },
    "code": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "materialId",
    "receivedDateTime",
    "documentId",
    "source",
    "documentType",
    "code"
  ],
  "additionalProperties": false
}
