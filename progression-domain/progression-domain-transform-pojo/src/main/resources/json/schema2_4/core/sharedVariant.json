{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/v24/core/courts/sharedVariant.json",
  "description": "Shared Variant",
  "type": "object",
  "properties": {
    "key": {
      "type": "object",
      "properties": {
        "hearingId": {
          "$ref": "http://justice.gov.uk/v24/core/courts/courtsDefinitions.json#/definitions/uuid"
        },
        "defendantId": {
          "$ref": "http://justice.gov.uk/v24/core/courts/courtsDefinitions.json#/definitions/uuid"
        },
        "nowsTypeId": {
          "$ref": "http://justice.gov.uk/v24/core/courts/courtsDefinitions.json#/definitions/uuid"
        },
        "usergroups": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "hearingId",
        "defendantId",
        "nowsTypeId",
        "usergroups"
      ]
    },
    "materialId": {
      "$ref": "http://justice.gov.uk/v24/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "templateName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "status": {
      "type": "string"
    }
  },
  "required": [
    "key",
    "materialId",
    "templateName",
    "description",
    "status"
  ]
}