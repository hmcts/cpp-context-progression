{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/v24/core/courts/material.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/v24/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "name": {
      "type": "string"
    },
    "uploadDateTime": {
      "type": "string",
	  "format": "date-time"
    },
    "generationStatus": {
      "type": "string"
    },
    "userGroups": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "id"
  ],
  "additionalProperties": false
}