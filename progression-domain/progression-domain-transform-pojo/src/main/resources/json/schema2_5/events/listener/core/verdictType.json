{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/verdictType.json",
  "description": "The type of verdict i.e. the type of court decision",
  "type": "object",
  "properties": {
	"id": {
	  "description": "The identifier of the verdict type from reference data",
	  "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
	},
	"sequence": {
	  "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/positiveInteger"
	},
	"description": {
	  "type": "string"
	},
	"category": {
	  "type": "string"
	},
	"categoryType": {
	  "type": "string"
	}
  },
  "additionalProperties": false,
  "required": [
	"id",
	"category",
	"categoryType"
  ]
}