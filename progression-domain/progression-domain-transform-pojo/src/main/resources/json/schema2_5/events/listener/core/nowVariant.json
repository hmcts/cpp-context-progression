{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/nowVariant.json",
  "description": "A now variant represents a variation of a single order i.e. a separate individual material for the same single order",
  "type": "object",
  "properties": {
	  "key": {
		"$ref": "http://justice.gov.uk/core/courts/nowVariantKey.json"
	  },
	  "nowVariantAddressee": {
		  "$ref": "http://justice.gov.uk/core/courts/nowVariantAddressee.json"
	  },
	  "nowVariantDefendant": {
		  "$ref": "http://justice.gov.uk/core/courts/nowVariantDefendant.json"
	  },
	  "materialId": {
		"$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
	  },
	  "templateName": {
		"type": "string"
	  },
	  "subTemplateName": {
		  "type": "string"
	  },
	  "description": {
		"type": "string"
	  },
	  "status": {
		"type": "string"
	  },
	  "isAmended": {
		"type": "boolean"
	  },
	  "isRemotePrintingRequired": {
		"type": "boolean"
	  },
	  "nowResults": {
		"type": "array",
		"minItems": 1,
		"items": {
			"$ref": "http://justice.gov.uk/core/courts/nowVariantResult.json"
		}
	  }
  },
  "additionalProperties": false,
  "required": [
	"key",
	  "materialId",
	  "templateName",
	  "description",
	  "status",
	  "nowVariantAddressee",
	  "nowVariantDefendant",
	  "isRemotePrintingRequired"
  ]
}