{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/nowdocument/prosecution-case.json",
  "description": "The case details and the associated defendant case results and defendant case offence results rendered in a NOW document",
  "type": "object",
  "properties": {
     "urn": {
	  "type": "string"
	},
	"defendantCaseResults": {
	  "type": "array",
	  "title": "defendantCaseResults",
	  "items": {
		"$ref": "http://justice.gov.uk/core/courts/nowdocument/result.json"
	  }
	},
	"defendantCaseOffences": {
	  "type": "array",
	  "title": "defendantCaseOffences",
	  "items": {
		"$ref": "http://justice.gov.uk/core/courts/nowdocument/defendant-case-offence.json"
	  }
	}
  },
  "required": [
    "urn",
    "defendantCaseOffences"
  ],
  "additionalProperties": false
}