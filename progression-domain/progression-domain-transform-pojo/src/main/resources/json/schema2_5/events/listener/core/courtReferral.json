{
	 "$schema": "http://json-schema.org/draft-04/schema#",
	 "id": "http://justice.gov.uk/core/courts/courtReferral.json",
	 "type": "object",
	 "properties": {
		 "prosecutionCases": {
			 "type": "array",
			 "minItems": 1,
			 "items": {
				 "$ref": "http://justice.gov.uk/core/courts/prosecutionCase.json"
			}
		},
		 "courtDocuments": {
			 "type": "array",
			 "minItems": 1,
			 "items": {
				 "$ref": "http://justice.gov.uk/core/courts/courtDocument.json"
			}
		},
		 "listHearingRequests": {
			 "type": "array",
			 "minItems": 1,
			 "items": {
				 "$ref": "http://justice.gov.uk/core/courts/listHearingRequest.json"
			}
		}
	},
	 "required": [
		"prosecutionCases",
		"listHearingRequests"
	],
	 "additionalProperties": false
}