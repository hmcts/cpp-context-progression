    },
    "prosecutionAuthorityId": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "prosecutionAuthorityName": {
      "type": "string"
    },
    "address": {
      "$ref": "http://justice.gov.uk/core/courts/address.json#"
    },
    "caseURN": {
      "type": "string"
    },
    "oldCpsProsecutor": {
      "description": "Old CPS Prosecutor for the case",
      "type": "string"
    },
    "majorCreditorCode": {
      "type": "string"
    },
    "prosecutionAuthorityOUCode": {
      "type": "string"
