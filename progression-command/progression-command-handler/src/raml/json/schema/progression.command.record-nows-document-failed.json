  "type": "object",
  "properties": {
    "payloadFileId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "templateIdentifier": {
      "type": "string"
    },
    "conversionFormat": {
      "type": "string"
    },
    "requestedTime": {
      "type": "string",
      "format": "date-time"
    },
    "failedTime": {
      "type": "string",
      "format": "date-time"
    },
    "materialId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "originatingSource": {
      "type": "string"
    },
    "reason": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "payloadFileId",
    "failedTime",
    "requestedTime",
    "templateIdentifier",
    "conversionFormat",
    "reason"
