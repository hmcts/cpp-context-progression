{
  "name": "LAADurableFunctions",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest --testPathIgnorePatterns ../Integration-Test/**",
    "it-test": "jest ../Integration-Test/.*",
    "func": "node -e \"require('child_process').spawn('/usr/local/bin/func', ['host', 'start'],{detached:true}, (err, stdot, std)=>{console.log(${err})}).unref(), require('sleep').sleep(10) & process.exit()\"",
    "post-func": "pkill -f func"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "azure-functions-core-tools": "^3.0.2245",
    "durable-functions": "^1.3.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "redis": "^3.0.2",
    "sinon": "^7.5.0",
    "win-ca": "^3.2.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "eslint-plugin-jest": "^23.0.5",
    "@satazor/eslint-config": "github:satazor/eslint-config",
    "jest": "^24.9.0",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "eslint": "^6.7.1",
    "mock-redis-client": "^0.91.13",
    "redis-mock": "^0.47.0",
    "shelljs": "^0.8.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
