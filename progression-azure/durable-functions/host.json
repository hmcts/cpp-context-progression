{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "tracing": {
    "consoleLevel": "verbose"
  },
  "queues": {
    "maxDequeueCount": 5,
    "newBatchThreshold": 8
  },
  "extensions": {
    "durableTask": {
      "hubName": "TaskHub"
    }
  }
}
