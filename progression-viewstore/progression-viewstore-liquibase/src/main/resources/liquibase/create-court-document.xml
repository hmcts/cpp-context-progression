<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="create-court-document" author="cpp.context.progression">
        <createTable tableName="court_document">
            <column name="id" type="UUID"/>
            <column name="payload" type="TEXT"/>
            <column name="prosecution_case_id" type="UUID"/>
        </createTable>
    </changeSet>
    <changeSet id="add-court_document-key-constraints" author="cpp.context.progression">

        <addPrimaryKey columnNames="id" constraintName="court_document_primary_key"
                       tableName="court_document"/>

    </changeSet>

</databaseChangeLog>
