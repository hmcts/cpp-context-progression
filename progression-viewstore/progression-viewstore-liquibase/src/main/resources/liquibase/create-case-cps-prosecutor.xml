<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="create-case-cps-prosecutor" author="cpp.context.progression">
        <createTable tableName="case_cps_prosecutor">
            <column name="case_id" type="UUID"  />
            <column name="old_cps_prosecutor" type="text" />
            <column name ="cps_prosecutor" type="text" />
        </createTable>
    </changeSet>
    <changeSet id="create-case-cps-prosecutor-key-constraints" author="cpp.context.progression">

        <addPrimaryKey columnNames="case_id" constraintName="case_cps_prosecutor_primary_key"
                       tableName="case_cps_prosecutor"/>

    </changeSet>
</databaseChangeLog>