<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="add-index-search-prosecution-case-reference" author="Salsa">
        <preConditions onFail="MARK_RAN">
            <not>
                <indexExists indexName="search_prosecution_case_idx_reference"
                             tableName="search_prosecution_case"/>
            </not>
        </preConditions>
        <createIndex tableName="search_prosecution_case"
                     indexName="search_prosecution_case_idx_reference">
            <column name="reference"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>
