{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/courts/progression/query/progression.query.partial-match-defendant.json",
  "type": "object",
  "properties": {
    "totalMatchedDefendants": {
      "description": "total count",
      "type": "integer"
    },
    "matchedDefendants": {
      "description": "",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "item": {
            "defendantId": {
              "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
            },
            "masterDefendantId": {
              "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
            },
            "caseReference": {
              "type": "string"
            },
            "courtProceedingsInitiated": {
              "type": "string",
              "format": "date-time"
            },
            "prosecutionCaseId": {
              "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
            },
            "firstName": {
              "type": "string"
            },
            "middleName": {
              "type": "string"
            },

            "lastName": {
              "type": "string"
            },
            "dateOfBirth": {
              "type": "string"
            },
            "pncId": {
              "type": "string"
            },
            "croNumber": {
              "type": "string"
            },
            "address": {
              "type": "object",
              "properties": {
                "addressLine1": {
                  "type": "string"
                },
                "addressLine2": {
                  "type": "string"
                },
                "addressLine3": {
                  "type": "string"
                },
                "addressLine4": {
                  "type": "string"
                },
                "addressLine5": {
                  "type": "string"
                },
                "postcode": {
                  "type": "string"
                }
              }
            },
            "defendantsMatchedCount": {
              "type": "integer"
            },
            "defendantsMatched": {
              "type": "array",
              "minItems": 1,
              "items": {
                "$ref": "#/definitions/defendant"
              }
            }
          }
        }
      }
    }
  },
  "required": [
    "defendantId",
    "masterDefendantId",
    "prosecutionCaseId",
    "courtProceedingsInitiated",
    "caseReference",
    "lastName"
  ],
  "definitions": {
    "defendant": {
      "type": "object",
      "properties": {
        "defendantId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "masterDefendantId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "caseReference": {
          "type": "string"
        },
        "courtProceedingsInitiated": {
          "type": "string",
          "format": "date-time"
        },
        "prosecutionCaseId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "firstName": {
          "type": "string"
        },
        "middleName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "dateOfBirth": {
          "type": "string"
        },
        "pncId": {
          "type": "string"
        },
        "croNumber": {
          "type": "string"
        },
        "address": {
          "type": "object",
          "properties": {
            "addressLine1": {
              "type": "string"
            },
            "addressLine2": {
              "type": "string"
            },
            "addressLine3": {
              "type": "string"
            },
            "addressLine4": {
              "type": "string"
            },
            "addressLine5": {
              "type": "string"
            },
            "postcode": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
