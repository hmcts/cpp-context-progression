#%RAML 0.8
title: Progression Query API
baseUri: http://localhost:8080/progression-query-api/query/api/rest/progression
protocols: [ HTTP, HTTPS ]

/material/{materialId}/metadata:
  description: get  material metadata
  get:
    description: |
      ...
      (mapping):
          responseType: application/vnd.progression.query.material-metadata+json
          name: progression.query.material-metadata
      ...
    responses:
      200:
        body:
          application/vnd.progression.query.material-metadata+json:
            schema: !include json/schema/progression.query.material-metadata.json
            example: !include json/progression.query.material-metadata.json

/material/{materialId}/content:
  description: get material content
  get:
    description: |
      ...
      (mapping):
          responseType: application/vnd.progression.query.material-content+json
          name: progression.query.material-content
      (mapping):
          responseType: application/vnd.progression.query.material-content-for-defence+json
          name: progression.query.material-content-for-defence
      (mapping):
          responseType: application/vnd.progression.query.material-content-for-prosecution+json
          name: progression.query.material-content-for-prosecution
      ...
      queryParameters:
          defendantId:
            type: string
            example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374
            required: false
          caseId:
            type: string
            example: 995f6482-ff86-4c3d-a9a0-f610ca34bd1c
            required: false
          applicationId:
            type: string
            example: 995f6482-ff86-4c3d-a9a0-f610ca34bd1c
            required: false
    responses:
      200:
        body:
          application/vnd.progression.query.material-content+json:
          application/vnd.progression.query.material-content-for-defence+json:
          application/vnd.progression.query.material-content-for-prosecution+json:

/prosecutioncases/{caseId}/defendants/{defendantId}/extract/{template}:
  description: get court extract content as pdf
  uriParameters:
    caseId:
      description: ID of prosecution case
      type: string
    defendantId:
      description: ID of the defendant
      type: string
    template:
      description: template name to pass to docmosis api(CrownCourtExtract, CertificateOfConviction or CertificateOfAcquittal)
      type: string
  get:
    description: |
      ...
      (mapping):
          responseType: application/vnd.progression.query.court-extract+json
          name: progression.query.court-extract
      ...
    queryParameters:
        hearingIds:
          description: comma separated list of hearingIds, only expected when template value is CrownCourtExtract
          type: string
          example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374,1f1fba3c-34ee-454e-b3be-5fb845b2de4a
          required: false
    responses:
      200:
        body:
          application/vnd.progression.query.court-extract+json:

/prosecutioncases/{caseId}:
  uriParameters:
        caseId:
          description: ID of prosecution case to get
          type: string
  get:
      description: |
        Return the case progression details for the prosecution case Id.
        ...
        (mapping):
            responseType: application/vnd.progression.query.case+json
            name: progression.query.case
        (mapping):
            responseType: application/vnd.progression.query.prosecutioncase+json
            name: progression.query.prosecutioncase
        (mapping):
                    responseType: application/vnd.progression.query.prosecutioncase-v2+json
                    name: progression.query.prosecutioncase-v2
        (mapping):
            responseType: application/vnd.progression.query.prosecutioncase.caag+json
            name: progression.query.prosecutioncase.caag
        (mapping):
            responseType: application/vnd.progression.query.casehearings+json
            name: progression.query.casehearings
        (mapping):
            responseType: application/vnd.progression.query.case.hearingtypes+json
            name: progression.query.case.hearingtypes
        (mapping):
            responseType: application/vnd.progression.query.case.allhearingtypes+json
            name: progression.query.case.allhearingtypes
        (mapping):
            responseType: application/vnd.progression.query.case.allhearings+json
            name: progression.query.case.allhearings
        (mapping):
            responseType: application/vnd.progression.query.prosecutioncase-details+json
            name: progression.query.prosecutioncase-details
        (mapping):
            responseType: application/vnd.progression.query.related-references+json
            name: progression.query.related-references
        ...
      queryParameters:
        orderDate:
          description: Order Date
          type: string
      responses:
            200:
                description: OK
                body:
                  application/vnd.progression.query.case+json:
                    example:  !include json/progression.query.case.json
                    schema:  !include json/schema/progression.query.case.json
                  application/vnd.progression.query.prosecutioncase+json:
                    example:  !include json/progression.query.prosecutioncase.json
                    schema:  !include json/schema/progression.query.prosecutioncase.json
                  application/vnd.progression.query.prosecutioncase-v2+json:
                    example:  !include json/progression.query.prosecutioncase.json
                    schema:  !include json/schema/progression.query.prosecutioncase.json
                  application/vnd.progression.query.prosecutioncase.caag+json:
                    example:  !include json/progression.query.prosecutioncase.caag.json
                    schema:  !include json/schema/progression.query.prosecutioncase.caag.json
                  application/vnd.progression.query.casehearings+json:
                    example:  !include json/progression.query.casehearings.json
                    schema:  !include json/schema/progression.query.casehearings.json
                  application/vnd.progression.query.case.hearingtypes+json:
                    example:  !include json/progression.query.case.hearingtypes.json
                    schema:  !include json/schema/progression.query.case.hearingtypes.json
                  application/vnd.progression.query.case.allhearingtypes+json:
                    example:  !include json/progression.query.case.allhearingtypes.json
                    schema:  !include json/schema/progression.query.case.allhearingtypes.json
                  application/vnd.progression.query.case.allhearings+json:
                    example: !include json/progression.query.case.allhearings.json
                    schema: !include json/schema/progression.query.case.allhearings.json
                  application/vnd.progression.query.prosecutioncase-details+json:
                    example:  !include json/progression.query.prosecutioncase-details.json
                    schema:  !include json/schema/progression.query.prosecutioncase-details.json
                  application/vnd.progression.query.related-references+json:
                    schema:  !include json/schema/progression.query.related-references.json

