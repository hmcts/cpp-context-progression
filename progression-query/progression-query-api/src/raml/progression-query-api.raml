/courtdocumentsearch:
  get:
      description: |
        Search for court documentIndices.UserDetailsLoaderTest
        ...
        (mapping):
            responseType: application/vnd.progression.query.courtdocuments+json
            name: progression.query.courtdocuments
        (mapping):
            responseType: application/vnd.progression.query.courtdocuments-all+json
            name: progression.query.courtdocuments-all
        (mapping):
            responseType: application/vnd.progression.query.courtdocuments-all+json
            name: progression.query.courtdocuments-all
        (mapping):
            responseType: application/vnd.progression.query.courtdocuments.for.defence+json
            name: progression.query.courtdocuments.for.defence
        (mapping):
            responseType: application/vnd.progression.query.courtdocuments.for.prosecution+json
            name: progression.query.courtdocuments.for.prosecution
        (mapping):
            responseType: application/vnd.progression.query.courtdocuments.with.pagination+json
            name: progression.query.courtdocuments.with.pagination
        ...
      queryParameters:
              caseId:
                description: caseId(s) - comma separated to be searched
                type: string
                example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374,1f1fba3c-34ee-454e-b3be-5fb845b2de4a
                required: false
              defendantId:
                description: defendant id to be searched
                type: string
                example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374,1f1fba3c-34ee-454e-b3be-5fb845b2de4a
                required: false
              applicationId:
                description: application id (s) - comma separated to be searched
                type: string
                example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374,1f1fba3c-34ee-454e-b3be-5fb845b2de4a
                required: false
              hearingId:
                description: Hearing id
                type: string
                example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374
                required: false
              documentName:
                description: name of the material
                type: string
                example: indictment-v1
                required: false
              sectionId:
                description: sectionId of the material
                type: string
                example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374
                required: false
              sortField:
                description:  the name of the field that sorting will be applied
                enum: [date,section]
                example: section
                type: string
                required: false
              sortOrder:
                description: sort direction
                enum: [asc, desc]
                example: asc
                type: string
                required: false
              page:
                description: page
                example: 2
                type: integer
                required: false
              pageSize:
                description: size
                example: 20
                type: integer
                required: false
              prosecutingCase:
                description: only applicable for standard courtlist. Set true to get restricted standard court list
                type: boolean
                required: false
                default: false
      responses:
            200:
                description: OK
                body:
                  application/vnd.progression.query.courtdocuments+json:
                    example:  !include json/progression.query.courtdocuments.json
                    schema:  !include json/schema/progression.query.courtdocuments.json
                  application/vnd.progression.query.courtdocuments-all+json:
                    example: !include json/progression.query.courtdocuments-all.json
                    schema: !include json/schema/progression.query.courtdocuments-all.json
                  application/vnd.progression.query.courtdocuments.for.defence+json:
                    example:  !include json/progression.query.courtdocuments.json
                    schema:  !include json/schema/progression.query.courtdocuments.json
                  application/vnd.progression.query.courtdocuments.for.prosecution+json:
                    example:  !include json/progression.query.courtdocuments.json
                    schema:  !include json/schema/progression.query.courtdocuments.json
                  application/vnd.progression.query.courtdocuments.with.pagination+json:
                    example:  !include json/progression.query.courtdocuments.with.pagination.json
                    schema:  !include json/schema/progression.query.courtdocuments.with.pagination.json

/search:
  get:
      description: |
        Search for cases
        ...
        (mapping):
            responseType: application/vnd.progression.query.search-cases+json
            name: progression.query.search-cases
        (mapping):
            responseType: application/vnd.progression.query.search-cases-by-caseurn+json
            name: progression.query.search-cases-by-caseurn
        (mapping):
            responseType: application/vnd.progression.query.case-exists-by-caseurn+json
            name: progression.query.case-exist-by-caseurn
        (mapping):
            responseType: application/vnd.progression.query.cases-search-by-material-id+json
            name: progression.query.cases-search-by-material-id
        (mapping):
            responseType: application/vnd.progression.query.usergroups-by-material-id+json
            name: progression.query.usergroups-by-material-id
        (mapping):
            responseType: application/vnd.progression.query.search-prosecution-authority-id+json
            name: progression.query.prosecutionauthorityid-by-case-id
        (mapping):
            responseType: application/vnd.progression.query.search-prosecutorid-prosecutionauthorityid+json
            name: progression.query.prosecutorid-prosecutionauthorityid-by-case-id
        ...
      queryParameters:
        q:
          description: space separated list of search parameters
          type: string
          example: first name last name
          required: false
        caseIds:
          description: caseId(s) - comma separated to be searched
          type: string
          example: b44fa9bb-dc36-4375-83a9-ff1bc4cd4374,1f1fba3c-34ee-454e-b3be-5fb845b2de4a
          required: false
        caseUrn:
          description: caseurn to search for
          type: string
          example: 39GD1116822
          required: false

      responses:
            200:
                description: OK
                body:
                  application/vnd.progression.query.search-cases+json:
                    example: !include json/progression.query.search-cases.json
                    schema: !include json/schema/progression.query.search-cases.json
                  application/vnd.progression.query.search-cases-by-caseurn+json:
                    example: !include json/progression.query.search-cases-by-caseurn.json
                    schema: !include json/schema/progression.query.search-cases-by-caseurn.json
                  application/vnd.progression.query.case-exists-by-caseurn+json:
                    example: !include json/progression.query.case-exists-by-caseurn.json
                    schema: !include json/schema/progression.query.case-exists-by-caseurn.json
                  application/vnd.progression.query.cases-search-by-material-id+json:
                    example: !include json/progression.query.cases-search-by-material-id.json
                    schema: !include json/schema/progression.query.cases-search-by-material-id.json
                  application/vnd.progression.query.usergroups-by-material-id+json:
                    example: !include json/progression.query.usergroups-by-material-id.json
                    schema: !include  json/schema/progression.query.usergroups-by-material-id.json
                  application/vnd.progression.query.search-prosecution-authority-id+json:
                    example: !include json/progression.query.prosecution-authority-id.json
                    schema: !include json/schema/progression.query.prosecution-authority-id.json
                  application/vnd.progression.query.search-prosecutorid-prosecutionauthorityid+json:
                    example: !include json/progression.query.prosecutorid-prosecutionauthorityid.json
                    schema: !include json/schema/progression.query.prosecutorid-prosecutionauthorityid.json

